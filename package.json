{
  "name": "next-js-starter",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "18",
    "yarn": "1"
  },
  "cacheDirectories": [
    "node_modules",
    ".next/cache"
  ],
  "scripts": {
    "dev": "yarn && NODE_OPTIONS='--no-warnings' next dev",
    "build": "NODE_ENV=production NODE_OPTIONS='--no-warnings' next build",
    "start": "NODE_ENV=production NODE_OPTIONS='--no-warnings' next start -p 3000",
    "heroku-postbuild": "npm run build",
    "generate:schema": "node src/prismic/generatePrismicIntrospection.js",
    "generate:types": "graphql-codegen --config codegen.yml",
    "generate": "yarn generate:schema && yarn generate:types",
    "lint:eslint": "eslint \"*.{js,ts,tsx}\" \"src/**/*.{js,ts,tsx}\" --max-warnings=0 --report-unused-disable-directives",
    "lint:stylelint": "stylelint \"src/**/*.scss\"",
    "lint:types": "tsc --noEmit --incremental false",
    "lint": "concurrently yarn:lint:*",
    "format:eslint": "eslint --fix \"*.{js,ts,tsx}\" \"src/**/*.{js,ts,tsx}\"",
    "format:prettier": "prettier --write \"*.{json,md}\" \"src/**/*.{json,md,html}\"",
    "format:stylelint": "prettier --write \"src/**/*.module.scss\"  && stylelint --fix \"src/**/*.module.scss\"",
    "format": "concurrently yarn:format:*",
    "test": "jest --silent",
    "slicemachine": "start-slicemachine"
  },
  "dependencies": {
    "@prismicio/client": "^6.7.1",
    "@prismicio/helpers": "^2.3.5",
    "@prismicio/react": "^2.5.0",
    "@prismicio/types": "^0.2.3",
    "@types/node": "^18.11.13",
    "@types/react": "^18.0.19",
    "@types/react-dom": "^18.0.6",
    "@types/react-transition-group": "^4.4.5",
    "@types/redis": "^2.8.28",
    "@types/tinycolor2": "^1.4.3",
    "classnames": "^2.3.1",
    "graphql": "^16.6.0",
    "next": "^13.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-fast-marquee": "^1.3.5",
    "react-intersection-observer": "^9.4.0",
    "react-slick": "^0.29.0",
    "react-transition-group": "^4.4.5",
    "redis": "^3.1.2",
    "sass": "^1.54.9",
    "sharp": "^0.31.2",
    "slick-carousel": "^1.8.1",
    "tinycolor2": "^1.6.0",
    "typescript": "^4.8.3"
  },
  "devDependencies": {
    "@babel/core": "^7.20.5",
    "@graphql-codegen/cli": "^2.12.0",
    "@graphql-codegen/introspection": "^2.2.1",
    "@graphql-codegen/typescript": "^2.7.3",
    "@graphql-codegen/typescript-operations": "^2.5.3",
    "@graphql-codegen/typescript-react-apollo": "^3.3.3",
    "@hugsmidjan/hxmstyle": "^6.3.0",
    "@next/eslint-plugin-next": "^13.1.1",
    "@types/react-slick": "^0.23.10",
    "@types/three": "^0.149.0",
    "@typescript-eslint/eslint-plugin": "^5.36.2",
    "@typescript-eslint/parser": "^5.36.2",
    "concurrently": "^7.4.0",
    "eslint": "^8.23.1",
    "eslint-config-next": "^13.1.1",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-deprecation": "^1.3.2",
    "eslint-plugin-destructure-depth": "^1.0.3",
    "eslint-plugin-destructuring": "^2.2.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-simple-import-sort": "^8.0.0",
    "eslint-plugin-total-functions": "^6.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "graphql-tag": "^2.12.6",
    "jest": "^29.0.3",
    "postcss": "^8.4.19",
    "prettier": "^2.7.1",
    "slice-machine-ui": "^0.6.0",
    "stylelint": "^14.11.0",
    "stylelint-config-recommended-scss": "^8.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.3.0",
    "ts-node": "^10.9.1"
  },
  "hxmstyle": {
    "options": {
      "react": true,
      "scss": true,
      "typescript": true
    },
    "dependenciesAdded": [
      "@typescript-eslint/eslint-plugin",
      "@typescript-eslint/parser",
      "eslint",
      "eslint-config-prettier",
      "eslint-plugin-deprecation",
      "eslint-plugin-destructure-depth",
      "eslint-plugin-destructuring",
      "eslint-plugin-import",
      "eslint-plugin-prettier",
      "eslint-plugin-react",
      "eslint-plugin-react-hooks",
      "eslint-plugin-simple-import-sort",
      "eslint-plugin-total-functions",
      "eslint-plugin-unused-imports",
      "prettier",
      "stylelint",
      "stylelint-config-recommended-scss",
      "stylelint-order",
      "stylelint-scss"
    ]
  }
}
